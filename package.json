{
  "name": "korenmern",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "productName": "Audio System",
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^6.2.0",
    "formidable": "^1.2.1",
    "peerjs": "^0.3.20",
    "pitchfinder": "^2.0.9",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "^2.1.5",
    "react-semantic-ui-range": "^0.6.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.85.0"
  },
  "scripts": {
    "start": "electron main.js",
    "build": "react-scripts build",
    "build-start": "npm run build && main.js",
